<html t:type="layout"  
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
      xmlns:p="tapestry:parameter">
      
    <p:title>${message:noteimport}</p:title>  

	<h2>${message:noteimport}</h2>

    <t:form t:id="uploadForm">
      <t:errors/>
      <input t:type="upload" t:id="file" t:validate="required"/>
      <input type="submit" value="${message:submit}"/>
    </t:form>
    
    <t:if test="message">
        <p class="msg">${message}</p>
    </t:if>

    <p:rightPanel>
        
    </p:rightPanel>

</html>
