<html t:type="layout"  
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
      xmlns:p="tapestry:parameter">
      
    <p:title>${message:documents}</p:title>  

    <ul>
    <t:loop source="documents" value="document">
        <li>
            <a t:type="pageLink" t:page="document/viewPage" t:context="${document.id}">${document.title}</a>
            <!-- 
            <ul class="actions">
                <li>
                    <a t:type="pageLink" t:page="document/annotatePage" t:context="${document.id}">${message:annotate-document}</a>
                </li>
            </ul>
             -->                   
        </li>    
    </t:loop>
    </ul>
        
    <p:eastPanel>
        <t:dashboard/>
    </p:eastPanel>

    <p:westPanel>
        west
    </p:westPanel>

</html>
