<html t:type="layout"  
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
      xmlns:p="tapestry:parameter">
      
    <p:title>${message:frontpage}</p:title>  
    
    <h3>${message:main-actions}</h3>

    <ul>
        <li>
            <a t:type="pageLink" t:page="documentsPage">${message:documents}</a>
        </li>                       
        <li>
            <a t:type="pageLink" t:page="noteSearchPage">${message:notesearch}</a>
        </li>
        <li>
            <a t:type="pageLink" t:page="noteImportPage">${message:noteimport}</a>
        </li>
    </ul>
    
    <p:breadcrumbs>
        ${message:frontpage}
    </p:breadcrumbs>
        
    <p:eastPanel>
        <t:dashboard/>
    </p:eastPanel>

    <p:westPanel>
        west
    </p:westPanel>

</html>
