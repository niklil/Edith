<html t:type="layout"  
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
      xmlns:p="tapestry:parameter">
      
    <p:title>${document.title}</p:title>  

    <div class="content">
        <t:document.view svnPath="${document.svnPath}" revision="literal:-1"/>        
    </div>
        
    <p:eastPanel>
        <t:dashboard/>
    </p:eastPanel>

    <p:westPanel>
        west
    </p:westPanel>

</html>
