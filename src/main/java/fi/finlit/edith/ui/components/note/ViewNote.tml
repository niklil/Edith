<html xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd">
	<t:content>
		<span>
		    <!-- <t:if test="note.lemma">${note.lemma}</t:if>-->
		    <t:if test="normalNote">
		         <t:if test="note.lemmaMeaning">, '${note.lemmaMeaning}'</t:if>
			     <t:if test="note.subtextSources">, ${message:compareSnippet} <t:paragraphFormatter value="note.subtextSources"/></t:if>
			</t:if>
			<t:if test="personNote">
			     <t:if test="person">
                    <t:if test="person.normalizedForm.first">, ${person.normalizedForm.first}</t:if>
                    <t:if test="person.normalizedForm.last">, ${person.normalizedForm.last}</t:if>
                    <t:if test="lifeDuration">, ${lifeDuration}</t:if>
                 </t:if>
            </t:if>
            <t:if test="placeNote">
                 <t:if test="place">
                    <t:if test="place.normalizedForm.name">, ${place.normalizedForm.name}</t:if>
                 </t:if>
            </t:if>
            <t:if test="note.description">, <t:paragraphFormatter value="note.description"/></t:if>
            <t:if test="note.sources">, (<t:paragraphFormatter value="note.sources"/>)</t:if>
		</span>
    </t:content>
</html>