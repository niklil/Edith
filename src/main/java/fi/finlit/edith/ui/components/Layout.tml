<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
      xmlns:p="tapestry:parameter">
    <head>
        <title><t:delegate to="title"/></title>
    </head>
    <body>
        <div id="container">
    
        <div id="header">         
          <div id="auth">
            <t:authPanel/>
          </div> 
          <div style="clear:both;"/>
        </div>
                
        <div id="top">
            <a t:type="pagelink" t:page="index" id="homeLink">
              <img src="${context:images/logo.jpg}"/>
            </a>
          <div class="title"><t:delegate to="title"/></div>
          <div style="clear:both;"/>
        </div>
        
        <table id="main">
          <tr>
            <td id="left"><t:delegate to="eastPanel"/></td>
            <td id="center" width="100%"><t:body/></td>
            <td id="right"><t:delegate to="westPanel"/></td>
          </tr>
        </table>
        
        <div id="bottom">
              <img src="${context:images/sks_logo.gif}"/>
        </div>      
        
        </div>
    </body>
</html>

